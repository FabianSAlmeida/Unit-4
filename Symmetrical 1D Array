//Created By Fabian Almeida
public class Arrays {

	public static void main(String[] args){
		
		//Printing Original Array
		System.out.println("Original Array : (2 3 15 15 3 2)");
		
        // Setting Original array to 2 exact arrays
		int arr1[] = {2, 3, 15, 15, 3, 2}; 
        int arr2[] = {2, 3, 15, 15, 3, 2}; 
  
        //if else statement to print if arrays are symmetrical
        if (equalArray(arr1, arr2)) 
            System.out.println("Yes, This array is not Symmertical"); 
        else
            System.out.println("No, This array is not Symmertical"); 
    }  
	
	//Method to see if Arrays are symmetrical
	public static boolean equalArray(int arr1[], int arr2[]) { 
	       
			//initializing array lengths with variables
			int n = arr1.length; 
	        int m = arr2.length; 
	 
	        //setting variables set to each other
	        if (n != m) 
	        	return false; 
	        else
	        	return true;
	    }
}
